PUBLIC INTERFACE:

value = 0:
value & = value[key]                                            : impl, json_test::test_operator_at_key
value & = value[index]                                          : impl, json_test::test_operator_at_index
const wchar_t * = value.get ()                                  : impl
const wchar_t * = value.stringify ()                            : impl

value = const value &                                           : impl, json_value_test_interface::assign_all_values
value = value *                                                 : impl  json_value_test::test_assign_value_ptr
value = double                                                  : impl, json_test
value = const wchar_t *json_input                               : impl, json_test
value = boolean                                                 : impl, json_test
value = nullptr                                                 : impl, json_test
value = undefined                                               : impl, json_test


json > value:
json (const wchar_t *json_input)                                : impl, json_test::test_ctor_dtor
json (object *)                                                 : impl, json_test::test_ctor_dtor
json (array *)s                                                 : impl, json_test::test_ctor_dtor
json (const json &)                                             : impl, json_test::test_ctor_dtor

json = const json &                                             : impl, json_value_test::test__clone_const_value_ref
json = const wchar_t *json_input                                : impl, no test
json = object *                                                 : ?
json = array *                                                  : ?


object > json:
object (const wchar_t *json_input)                              : impl
object (const object &)                                         : impl
object {{std::wstring, value *},{std::wstring, value *}}        : impl

object = const object &                                         : impl
object = const wchar_t *json_input                              : impl
object = {{std::wstring, value *},{std::wstring, value *}}      : ?

array > json:
array (const wchar_t *json_input)                               : impl
array (const array &)                                           : impl
array {value*, value*...}                                       : impl

array = const array &                                           : impl
array = const wchar_t *json_input                               :
array = {value*, value*...}                                     : ?

number > leaf > value:
number (double)                                                 : impl
number (float)                                                  : impl
number (const number &)                                         : impl

number = double                                                 : impl
number = float                                                  : impl
number = const number &                                         : impl


string > leaf:
string (const wchar_t *)                                        : impl
string (const number &)                                         : impl

string  = const wchar_t *                                       : impl
string  = const string &                                        : impl


boolean > leaf:
boolean (bool)                                                  : impl
boolean (const boolean &)                                       : impl

boolean = bool                                                  : impl
boolean = const boolean &                                       : impl


null > leaf:
null                                                            : impl


